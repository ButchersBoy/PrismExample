<UserControl x:Class="ModuleOne.Views.ViewThree"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ModuleOne.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top">
            <Label Content="This is the text you passed:"/>
            <TextBox Text="{Binding ParameterExample}"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Navigate to View One" HorizontalAlignment="Center" Margin="5" Command="{Binding NavigateToViewOneCommand}"/>
            <Button Content="Navigate to View One And Close" HorizontalAlignment="Center" Margin="5" Command="{Binding NavigateToViewOneAndCloseCommand}"/>
            <Button Content="Navigate to View Two" HorizontalAlignment="Center" Margin="5" Command="{Binding NavigateToViewTwoCommand}"/>
            <Button Content="Navigate to View Two And Close" HorizontalAlignment="Center" Margin="5" Command="{Binding NavigateToViewTwoAndCloseCommand}"/>
        </StackPanel>
    </Grid>
</UserControl>
